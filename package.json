{
  "name": "nextjs-with-styled-components-typescript",
  "version": "5.0.0",
  "private": true,
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,ts,tsx, jsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,md,html,js,jsx,ts,tsx}": [
      "npx prettier --write ."
    ],
    "*": [
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn check-all"
    }
  },
  "scripts": {
    "prepare": "husky install .husky",
    "test": "jest --passWithNoTests",
    "check-format": "prettier --check .",
    "check-types": "tsc --pretty --noEmit",
    "check-all": "yarn prettier && yarn lint && yarn check-format && yarn check-types",
    "dev": "rm -rf .next && NODE_ENV=test next dev",
    "build": "rm -rf .next && next build",
    "lint": "lint-staged",
    "prettier": "prettier --write .",
    "development": "NODE_ENV=development yarn build && next start",
    "production": "NODE_ENV=production yarn build && next start",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.5",
    "@mui/material": "latest",
    "@mui/styled-engine-sc": "latest",
    "classnames": "^2.3.1",
    "graphql": "^16.0.0",
    "graphql-request": "^3.6.1",
    "next": "latest",
    "next-auth": "^3.29.5",
    "next-pwa": "^5.4.0",
    "next-transpile-modules": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-hook-form": "^7.18.0",
    "react-is": "latest",
    "react-swipeable-views": "^0.14.0",
    "react-swipeable-views-utils": "^0.14.0",
    "styled-components": "latest"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@mui/styles": "^5.0.2",
    "@types/js-cookie": "^3.0.0",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-swipeable-views": "^0.13.1",
    "@types/react-swipeable-views-utils": "^0.13.3",
    "@types/styled-components": "latest",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "babel-jest": "^27.3.1",
    "babel-plugin-styled-components": "^1.13.3",
    "eslint": "<8.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "^12.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "sharp": "^0.29.2",
    "typescript": "^4.4.4"
  }
}
